{
	"name": "ultrascn/admin",
	"type": "library",
	"description": "Administration extension for Nette.",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Jan Pecha",
			"homepage": "https://www.janpecha.cz/"
		}
	],
	"funding": [
		{"type": "other", "url": "https://www.janpecha.cz/donate/"}
	],
	"require": {
		"php": ">=5.6.0",
		"czproject/assert": "^1.3",
		"inteve/application": "^1.0",
		"inteve/assets-manager": "^0.5",
		"inteve/navigation": "^3.4",
		"latte/latte": "^2.4",
		"nette/application": "^2.4",
		"nette/http": "^2.4",
		"nette/security": "^2.4",
		"typro/bridge-nette-forms": "^1.0",
		"typro/typro": "^3.3",
		"inteve/forms": "^1.0 || ^2.0",
		"inteve/datagrid": "^0.4 || ^1.0"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"require-dev": {
		"nette/bootstrap": "^2.4",
		"nette/di": "^2.4",
		"nette/tester": "^2.0"
	},
	"extra": {
		"assets-files": "assets/",
		"cleaner-ignore": [
			"assets"
		],
		"phpstan-extensions": [
			"phpstan/phpstan-nette"
		]
	}
}
